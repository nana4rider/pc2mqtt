              "ipAddress": { "type": "string" },
              "subnetMask": { "type": "string" }
            },
            "required": ["ipAddress", "macAddress", "ssh"]
          }
        },
        "required": ["id", "name", "remote"]
      }
    }
  },
  "required": ["deviceId", "entities"],
  "definitions": {
    "Config": {
      "allOf": [
        { "$ref": "#/definitions/ConnectConfig" },
        {
          "type": "object",
          "properties": {
            "password": { "type": "string" },
            "privateKey": { "type": "string" },
            "privateKeyPath": { "type": "string" },
            "tryKeyboard": { "type": "boolean" },
            "onKeyboardInteractive": { "type": "object" }
          }
        }
      ]
    },
    "ConnectConfig": {
      "type": "object",
      "properties": {
        "host": {
          "description": "Hostname or IP address of the server.",
          "type": "string"
        },
        "port": {
          "description": "Port number of the server.",
          "type": "number"
        },
        "forceIPv4": {
          "description": "Only connect via resolved IPv4 address for `host`.",
          "type": "boolean"
        },
        "forceIPv6": {
          "description": "Only connect via resolved IPv6 address for `host`.",
          "type": "boolean"
        },
        "hostHash": {
          "description": "The host's key is hashed using this method and passed to `hostVerifier`.",
          "type": "string"
        },
        "hostVerifier": {
          "description": "Verifies a hexadecimal hash of the host's key.",
          "type": "object"
        },
        "username": {
          "description": "Username for authentication.",
          "type": "string"
        },
        "password": {
          "description": "Password for password-based user authentication.",
          "type": "string"
        },
        "agent": {
          "description": "Path to ssh-agent's UNIX socket for ssh-agent-based user authentication (or 'pageant' when using Pagent on Windows).",
          "anyOf": [
            { "$ref": "#/definitions/BaseAgent<string|Buffer|ParsedKey>" },
            { "type": "string" }
          ]
        },
        "privateKey": {
          "description": "Buffer or string that contains a private key for either key-based or hostbased user authentication (OpenSSH format).",
          "anyOf": [
            {
              "type": "object",
              "additionalProperties": false,
              "patternProperties": { "^[0-9]+$": { "type": "number" } },
              "properties": {
                "BYTES_PER_ELEMENT": { "type": "number" },
                "buffer": { "$ref": "#/definitions/ArrayBufferLike" },
                "byteLength": { "type": "number" },
                "byteOffset": { "type": "number" },
                "length": { "type": "number" },
                "__@toStringTag@266": {
                  "type": "string",
                  "const": "Uint8Array"
                }
              },
              "required": [
                "BYTES_PER_ELEMENT",
                "__@toStringTag@266",
                "buffer",
                "byteLength",
                "byteOffset",
                "length"
              ]
            },
            { "type": "string" }
          ]
        },
        "passphrase": {
          "description": "For an encrypted private key, this is the passphrase used to decrypt it.",
          "anyOf": [
            {
              "type": "object",
              "additionalProperties": false,
              "patternProperties": { "^[0-9]+$": { "type": "number" } },
              "properties": {
                "BYTES_PER_ELEMENT": { "type": "number" },
                "buffer": { "$ref": "#/definitions/ArrayBufferLike" },
                "byteLength": { "type": "number" },
                "byteOffset": { "type": "number" },
                "length": { "type": "number" },
                "__@toStringTag@266": {
                  "type": "string",
                  "const": "Uint8Array"
                }
              },
              "required": [
                "BYTES_PER_ELEMENT",
                "__@toStringTag@266",
                "buffer",
                "byteLength",
                "byteOffset",
                "length"
              ]
            },
            { "type": "string" }
          ]
        },
        "localHostname": {
          "description": "Along with `localUsername` and `privateKey`, set this to a non-empty string for hostbased user authentication.",
          "type": "string"
        },
        "localUsername": {
          "description": "Along with `localHostname` and `privateKey`, set this to a non-empty string for hostbased user authentication.",
          "type": "string"
        },
        "tryKeyboard": {
          "description": "Try keyboard-interactive user authentication if primary user authentication method fails.",
          "type": "boolean"
        },
        "keepaliveInterval": {
          "description": "How often (in milliseconds) to send SSH-level keepalive packets to the server. Set to 0 to disable.",
          "type": "number"
        },
        "keepaliveCountMax": {
          "description": "How many consecutive, unanswered SSH-level keepalive packets that can be sent to the server before disconnection.",
          "type": "number"
        },
        "readyTimeout": {
          "description": "* How long (in milliseconds) to wait for the SSH handshake to complete.",
          "type": "number"
        },
        "strictVendor": {
          "description": "Performs a strict server vendor check before sending vendor-specific requests.",
          "type": "boolean"
        },
        "sock": {
          "description": "A `ReadableStream` to use for communicating with the server instead of creating and using a new TCP connection (useful for connection hopping).",
          "$ref": "#/definitions/Readable"
        },
        "agentForward": {
          "description": "Set to `true` to use OpenSSH agent forwarding (`auth-agent@openssh.com`) for the life of the connection.",
          "type": "boolean"
        },
        "algorithms": {
          "description": "Explicit overrides for the default transport layer algorithms used for the connection.",
          "$ref": "#/definitions/Algorithms"
        },
        "debug": {
          "description": "A function that receives a single string argument to get detailed (local) debug information.",
          "type": "object"
        },
        "authHandler": {
          "description": "Function with parameters (methodsLeft, partialSuccess, callback) where methodsLeft and partialSuccess are null on the first authentication attempt, otherwise are an array and boolean respectively. Return or call callback() with the name of the authentication method to try next (pass false to signal no more methods to try). Valid method names are: 'none', 'password', 'publickey', 'agent', 'keyboard-interactive', 'hostbased'. Default: function that follows a set method order: None -> Password -> Private Key -> Agent (-> keyboard-interactive if tryKeyboard is true) -> Hostbased.",
          "anyOf": [
            {
              "type": "array",
              "items": { "$ref": "#/definitions/AuthenticationType" }
            },
            {
              "type": "array",
              "items": { "$ref": "#/definitions/AuthMethod" }
            },
            { "type": "object" }
          ]
        },
        "localAddress": {
          "description": "IP address of the network interface to use to connect to the server. Default: (none -- determined by OS)",
          "type": "string"
        },
        "localPort": {
          "description": "The local port number to connect from. Default: (none -- determined by OS)",
          "type": "number"
        },
        "timeout": {
          "description": "The underlying socket timeout in ms. Default: none)",
          "type": "number"
        },
        "ident": {
          "description": "A custom server software name/version identifier. Default: 'ssh2js' + moduleVersion + 'srv'",
          "anyOf": [
            {
              "type": "object",
              "additionalProperties": false,
              "patternProperties": { "^[0-9]+$": { "type": "number" } },
              "properties": {
                "BYTES_PER_ELEMENT": { "type": "number" },
                "buffer": { "$ref": "#/definitions/ArrayBufferLike" },
                "byteLength": { "type": "number" },
                "byteOffset": { "type": "number" },
                "length": { "type": "number" },
